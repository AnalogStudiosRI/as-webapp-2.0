<!DOCTYPE html>
<html lang="en">

  <head>

    <title>Analog Studios : Music and Recordings | {{page.title}}</title>
    {{> icon-link-tag }}

    <!-- App CSS -->
    {{#production}}
      {{#globals.app.prod.stylesheets}}
        {{> style-link-tag }}
      {{/globals.app.prod.stylesheets}}
    {{/production}}
    {{^production}}
      {{#globals.app.dev.stylesheets}}
        {{> style-link-tag }}
      {{/globals.app.dev.stylesheets}}
    {{/production}}

      <!-- Page CSS -->
    {{#page.stylesheets}}
      {{> style-link-tag}}
    {{/page.stylesheets}}

    <!-- Page CSS -->
    {{#page.stylesheets}}
      {{> style-link-tag}}
    {{/page.stylesheets}}

  </head>

  <body data-ng-cloak data-ng-app="{{page.app}}" data-ng-strict-di>

    <!-- header -->
    {{> header-admin }}

    <!-- Page Output -->
    <!-- Page (controller) -->
    <section class="row" id="{{page.id}}" data-ng-controller="{{page.ctrl}}">
      {{> body }}
    </section>

    <!-- App JS -->
    {{#production}}
      {{#globals.app.prod.scripts}}
        {{> script-tag }}
      {{/globals.app.prod.scripts}}
    {{/production}}
    {{^production}}
      {{#globals.app.dev.scripts}}
        {{> script-tag }}
      {{/globals.app.dev.scripts}}
    {{/production}}

    <!-- Page JS -->
    {{#page.scripts}}
      {{> script-tag }}
    {{/page.scripts}}

  </body>
</html>
